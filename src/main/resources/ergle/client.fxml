<?xml version="1.0" encoding="UTF-8"?>

<?scenebuilder-classpath-element ../../../src?>
<?scenebuilder-stylesheet main.css?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane prefHeight="668.0" prefWidth="437.0" top="$null" xmlns:fx="http://javafx.com/fxml/1"
            xmlns="http://javafx.com/javafx/2.2" fx:controller="ergle.ClientController">
    <center>
        <VBox prefHeight="353.0" prefWidth="223.0" spacing="7.0" styleClass="body">
            <children>
                <Label text="Add or remove folders from ergle" underline="false" wrapText="true"/>
                <Button alignment="CENTER_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#browse"
                        styleClass="controlButton" text="Add folder" textAlignment="LEFT"/>
                <ScrollPane fx:id="watchedPathsContainer" content="$null" fitToHeight="true" fitToWidth="true"
                            hbarPolicy="NEVER" pannable="false" prefHeight="200.0" prefWidth="200.0"
                            VBox.vgrow="ALWAYS"/>
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" fx:id="x1"/>
            </padding>
        </VBox>
    </center>
</BorderPane>
